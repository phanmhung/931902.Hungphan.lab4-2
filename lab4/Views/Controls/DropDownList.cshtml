@{
    ViewData["Title"] = "DropDownList";
}

<h1>@ViewData["Title"]</h1>
<form method="post">
    <div><b>Month</b></div>
    @Html.DropDownList("myList",
    new SelectList(new[] { "January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December" }), "Select")
    
    <p>
        <input type="submit" class="btn btn-outline-dark" value="Submit" />
    </p>
</form>